<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>Fluent Package v<%= pkg_version %></title>
    <background file="background.png" alignment="bottomleft" mime-type="image/png"/>
    <license file="license.rtf" mime-type="text/html"/>
    <welcome file="welcome.html" mime-type="text/html"/>
    <pkg-ref id="com.treasuredata.tdagent"/>
    <options require-scripts="false"/>
    <choices-outline>
        <line choice="default">
            <line choice="com.treasuredata.tdagent"/>
        </line>
    </choices-outline>
    <choice id="default" title="default"/>
    <choice id="com.treasuredata.tdagent" visible="false" title="com.treasuredata.tdagent">
        <pkg-ref id="com.treasuredata.tdagent"/>
    </choice>
    <pkg-ref id="com.treasuredata.tdagent" version="<%= version %>" onConclusion="none">fluent-package.pkg</pkg-ref>
</installer-gui-script>
